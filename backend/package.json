{
    "name":  "@edutech/backend",
    "version":  "0.1.0",
    "private":  true,
    "type":  "commonjs",
    "main":  "dist/server.js",
    "scripts":  {
                    "dev":  "ts-node-dev --respawn --transpile-only src/server.ts",
                    "build":  "tsc -p tsconfig.json",
                    "typecheck":  "tsc -p tsconfig.json --noEmit",
                    "postinstall":  "prisma generate",
                    "prisma":  "prisma",
                    "db:push":  "prisma db push",
                    "db:migrate":  "prisma migrate dev --name init",
                    "db:studio":  "prisma studio"
                },
    "dependencies":  {
                         "@aws-sdk/client-s3":  "^3.645.0",
                         "@edutech/types":  "file:../packages/types",
                         "@prisma/client":  "^5.18.0",
                         "bcryptjs":  "^2.4.3",
                         "cors":  "^2.8.5",
                         "dotenv":  "^16.4.5",
                         "express":  "^4.18.2",
                         "helmet":  "^7.1.0",
                         "jsonwebtoken":  "^9.0.2",
                         "mime-types":  "^2.1.35",
                         "morgan":  "^1.10.0",
                         "multer":  "^1.4.5-lts.1",
                         "swagger-ui-express":  "^5.0.0",
                         "yaml":  "^2.4.2",
                         "zod":  "^3.22.4",
                         "handlebars":  "^4.7.8",
                         "mammoth":  "^1.5.1",
                         "ajv":  "^8.12.0",
                         "openai":  "^4.52.0",
                         "puppeteer":  "^22.7.1"
                     },
    "devDependencies":  {
                            "@types/bcryptjs":  "^3.0.0",
                            "@types/cors":  "^2.8.17",
                            "@types/express":  "^4.17.21",
                            "@types/jsonwebtoken":  "^9.0.6",
                            "@types/mime-types":  "^2.1.4",
                            "@types/morgan":  "^1.9.6",
                            "@types/multer":  "^1.4.12",
                            "@types/node":  "^20.11.24",
                            "@types/swagger-ui-express":  "^4.1.7",
                            "prisma":  "^5.18.0",
                            "ts-node-dev":  "^2.0.0",
                            "typescript":  "^5.3.3"
                        },
    "pnpm":  {
                 "patchedDependencies":  {

                                         }
             },
    "preferUnplugged":  false,
    "packageManager":  "npm@10"
}
